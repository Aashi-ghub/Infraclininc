{
  "name": "backendbore",
  "version": "1.0.0",
  "description": "Infrastructure-grade geological logging backend",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "npm run start:dev",
    "start": "AWS_REGION=us-east-1 IS_OFFLINE=true PARQUET_LAMBDA_FUNCTION_NAME=parquet-repository-dev-parquet-repository serverless offline --httpPort ${PORT:-3000} --host 0.0.0.0",
    "start:dev": "set AWS_REGION=us-east-1 && set IS_OFFLINE=false && set PARQUET_LAMBDA_FUNCTION_NAME=parquet-repository-dev-parquet-repository && serverless offline",
    "start:prod": "AWS_REGION=us-east-1 IS_OFFLINE=true PARQUET_LAMBDA_FUNCTION_NAME=parquet-repository-dev-parquet-repository serverless offline --httpPort ${PORT:-3000} --host 0.0.0.0",
    "deploy": "serverless deploy",
    "test": "jest",
    "test:unit": "jest tests/unit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.370.0",
    "@aws-sdk/client-secrets-manager": "^3.370.0",
    "@aws-sdk/client-sqs": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.370.0",
    "bcryptjs": "^3.0.2",
    "busboy": "^1.6.0",
    "csv-parse": "^6.1.0",
    "exceljs": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.1",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@serverless/typescript": "^4.17.1",
    "@types/aws-lambda": "^8.10.130",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.8",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.48.5",
    "serverless-offline": "^13.3.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
